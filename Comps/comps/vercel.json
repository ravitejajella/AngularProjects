{
  "version": 2,
  "routes": [
    {
      "src": "/elements(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/views(.*)",
      "dest": "/views-home.html"
    },
    {
      "src": "/collections(.*)",
      "dest": "/collection-home.html"
    },
    {
      "src": "/mods(.*)",
      "dest": "/mods-home.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 404
    }
  ]
}